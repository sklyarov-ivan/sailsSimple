<div class="container">
  <h1>Sign up</h1>
    <% if (flash && flash.err) { %>
    <ul>
      <% Object.keys(flash.err).forEach(function(error){ %>
        <il><%- JSON.stringify(flash.err[error]) %></li>
      <% }) %>
    </ul>
    <% } %>
  <form class="" method="POST" action="/user/create">
    <div class="form-group">
      <input type="text" name="name" id="" placeholder="Enter Name">
    </div>
    <div class="form-group">
      <input type="text" name="title" id="" placeholder="Enter Title">
    </div>
    <div class="form-group">
      <input type="email" name="email" id="" placeholder="Enter email">
    </div>
    <div class="form-group">
      <input type="password" name="password" id="" placeholder="Enter password">
    </div>
    <div class="form-group">
      <input type="password" name="" id="password_conf" placeholder="Confirm password">
    </div>
    <input type="submit" class="btn">
    <input type="hidden" name="_csrf" value="<%= _csrf %>" />

  </form>
</div>